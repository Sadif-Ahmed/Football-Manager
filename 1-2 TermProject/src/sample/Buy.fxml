                  <TableColumn fx:id="Salary" prefWidth="125.60001220703123" text="Weekly salary" />
               </columns>
            </TableView>
            <Label layoutX="175.0" layoutY="41.0" text="Transfer Window(Buy)" textFill="#dd7272">
